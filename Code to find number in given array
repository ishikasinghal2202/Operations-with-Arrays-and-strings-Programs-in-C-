#include <iostream>
using namespace std;

int main() {
     int arr[] = {10, 20,30, 40,50}, target;
     cout << "Enter number to search: ";
     cin >> target;
     
     for (int i = 0; i < 5; i++) 
         if(arr[i] == target)
            return cout << "Found at index " <<i, 0;
            
    cout<< "Not found";
    return 0;
}
